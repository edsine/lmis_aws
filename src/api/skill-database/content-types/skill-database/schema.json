{
  "kind": "collectionType",
  "collectionName": "skill_databases",
  "info": {
    "singularName": "skill-database",
    "pluralName": "skill-databases",
    "displayName": "Skill Database"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "sector": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sector.sector"
    },
    "skill_status": {
      "type": "enumeration",
      "enum": [
        "In Demand",
        "Outdated",
        "Projected"
      ]
    }
  }
}
